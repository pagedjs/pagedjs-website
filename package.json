{
  "name": "web-book-maker",
  "version": "0.0.1",
  "description": "an eleventy template for book on screen",
  "main": ".eleventy.js",
  "type": "module",
  "scripts": {
    "build": "eleventy",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve --incremental",
    "start": "eleventy --serve --incremental --quiet",
    "debug": "DEBUG=* eleventy"
  },
  "keywords": [
    "book",
    "screen-reading",
    "static-site-generator"
  ],
  "author": "julientaq",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^3.1.1",
    "@11ty/eleventy-fetch": "^5.1.0",
    "@11ty/eleventy-img": "^6.0.4",
    "browserslist": "^4.25.1",
    "eleventy-plugin-i18n-helpers": "^0.0.2",
    "eleventy-plugin-toc": "^1.1.5",
    "lightningcss": "^1.30.1",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "slug": "^11.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.2.2",
    "eleventy-plugin-nesting-toc": "^1.3.0",
    "lightningcss-cli": "^1.30.1"
  },
  "volta": {
    "node": "20.19.3"
  }
}
