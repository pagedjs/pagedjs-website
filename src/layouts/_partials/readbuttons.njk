<aside class="readMenu">

    <button id="nightmode">
        <span class="icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="5"/><path d="M12 1v2M12 21v2M4.2 4.2l1.4 1.4M18.4 18.4l1.4 1.4M1 12h2M21 12h2M4.2 19.8l1.4-1.4M18.4 5.6l1.4-1.4"/></svg>
        </span>
        <span class='label'>night-mode</span>
    </button>

    <div class="zooms">
        <button id="zoom">
            <span class="icon">

                <svg width="24" height="24" viewBox="0 0 24 24" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                    <path d="M12.9538 23H15L8 7L1 23H3.04615L4.96308 18.4286H11.0369L12.9538 23ZM8 11.2014L10.3046 16.6871H5.69538L8 11.2014Z"/>
                    <path fill-rule="evenodd" clip-rule="evenodd" d="M19.8025 8.845V12.245H18.4425L18.4425 8.845V8.8025H18.4H15V7.4425L18.4 7.4425H18.4425V7.4L18.4425 4H19.8025L19.8025 7.4V7.4425H19.845H23.245V8.8025L19.845 8.8025H19.8025V8.845Z"/>
                    <path d="M15 15.605H18.4L19.845 15.605L23.245 15.605V14.245H19.845L18.4 14.245L15 14.245V15.605Z"/>
                </svg>

            </span>
            <span class="label">zoom</span>
        </button>
        <div class="zoomcontrols">
            <button id="zoomplus">
                <span class="icon">
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="currentColor"  xmlns="http://www.w3.org/2000/svg">
                        <path d="M12.9538 23H15L8 7L1 23H3.04615L4.96308 18.4286H11.0369L12.9538 23ZM8 11.2014L10.3046 16.6871H5.69538L8 11.2014Z"/>
                        <path fill-rule="evenodd" clip-rule="evenodd" d="M19.8025 10.845V14.245H18.4425L18.4425 10.845V10.8025H18.4H15V9.4425L18.4 9.4425H18.4425V9.4L18.4425 6H19.8025L19.8025 9.4V9.4425H19.845H23.245V10.8025L19.845 10.8025H19.8025V10.845Z"/>
                    </svg>

                </span>
                <span class="label">magnify</span>
            </button>
            <button id="zoomminus">
                <span class="icon">
                    <svg width="24" height="24"  viewBox="0 0 24 24"  fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path d="M13.8077 23H16L8.5 7L1 23H3.19231L5.24615 18.4286H11.7538L13.8077 23ZM8.5 11.2014L10.9692 16.6871H6.03077L8.5 11.2014Z"/>
                        <path d="M14 10.36H17.4L18.845 10.36L22.245 10.36V9H18.845L17.4 9L14 9V10.36Z"/>
                    </svg>

                </span>
                <span class="label">shrink</span>
            </button>
            <button id="zoomnone">
                <span class="icon">
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path d="M13.9538 23H16L9 7L2 23H4.04615L5.96308 18.4286H12.0369L13.9538 23ZM9 11.2014L11.3046 16.6871H6.69538L9 11.2014Z"/>
