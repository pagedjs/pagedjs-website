{% extends "../pages/singlepage.njk" %}
{% from "../addPaginator.njk" import defaultPaginator with context %}
{% block content %}
    {{ super() }}
    <aside>
        This page is also available in:
        {% for link in page.url | locale_links %}
            {%- if not loop.first %},{% endif %}
            <a href="{{ link.url }}"
               lang="{{ link.lang }}"
               hreflang="{{ link.lang }}">{{ link.label }}</a>
        {% endfor %}
    </aside>
    {{ defaultPaginator(collections.documentation) }}
{% endblock %}
